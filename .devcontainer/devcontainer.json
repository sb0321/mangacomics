{
	"name": "devcontainer-mangacomics-java",
	"dockerComposeFile": "docker-compose.yml",
	"features": {
		"ghcr.io/devcontainers/features/java:1": {
			"version": "21",
			"installMaven": "false",
			"installGradle": "true"
		}
	},
	"customizations": {
		"vscode": {
			"extensions": [
				"vscjava.vscode-java-pack",
				"mhutchie.git-graph",
				"github.copilot",
				"github.vscode-pull-request-github",
				"ms-ceintl.vscode-language-pack-ko"
			]
		}
	},
	"workspaceFolder": "/workspace/mangacomics"
}
